{
  "name": "isapp",
  "private": true,
  "workspaces": [
    "front",
    "back",
    "landing"
  ],
  "version": "0.5.0",
  "license": "MIT",
  "scripts": {
    "types": "tcm front && tsc --noEmit -p front && tsc --noEmit -p back",
    "circular": "madge --circular --extensions ts ./ --tsConfig tsconfig.json",
    "test": "jest",
    "s": "yarn soda"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "tsconfig-paths": "^3.7.0",
    "tsoption": "^0.7.0"
  },
  "devDependencies": {
    "@solid-soda/scripts": "^1.14.2",
    "@types/jest": "^24.0.14",
    "fast-check": "^1.15.1",
    "jest": "^24.8.0",
    "jest-module-name-mapper": "^0.1.2",
    "madge": "^3.4.3",
    "pm2": "^3.2.4",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn soda lint-staged",
      "pre-push": "yarn soda lint && yarn types && yarn circular && yarn types",
      "commit-msg": "yarn soda commitlint"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
